{
  "name": "member-api",
  "version": "2.1.3",
  "description": "Team Vegan.at Member API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "lint": "yarn run prettier:check && yarn run eslint",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t member-api .",
    "docker:run": "docker run -p 3000:3000 -d member-api",
    "premigrate": "yarn build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "yarn run clean && yarn run build",
    "prestart": "yarn run rebuild",
    "start": "node -r dotenv/config -r source-map-support/register .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "Vegane Gesellschaft Oesterreich, Team Vegan.at",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.3.2",
    "@loopback/boot": "^3.4.2",
    "@loopback/context": "^3.17.1",
    "@loopback/core": "^2.16.2",
    "@loopback/repository": "^3.7.1",
    "@loopback/rest": "^10.0.0",
    "@loopback/rest-explorer": "^3.3.2",
    "@loopback/service-proxy": "^3.2.2",
    "@mailchimp/mailchimp_marketing": "^3.0.58",
    "@mollie/api-client": "^3.5.1",
    "axios": "^0.21.1",
    "cron": "^1.8.2",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "moment": "^2.29.1",
    "node-redis-retry-strategy": "^2.1.1",
    "node-redis-scan": "^1.3.2",
    "redis": "^3.1.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.0",
    "@loopback/eslint-config": "^11.0.0",
    "@loopback/testlab": "^3.4.2",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^16.6.1",
    "@types/redis": "^2.8.31",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "source-map-support": "^0.5.19",
    "typescript": "~4.3.5"
  }
}
